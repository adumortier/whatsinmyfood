<h1> <%= @foods.count %> results </h1>

<% @foods.each do |food| %>
  <ul class='food'>
    <li class='gtin_upc_code'> GTIN/UPC code: <%= food[:gtinUpc] %> </li>
    <li class='description'> Description: <%= food[:description] %></li>
    <li class='brand_owner'> Brand Owner: <%= food[:brandOwner] %></li>
    <li class='ingredients'> Ingredients: <%= food[:ingredients] %></li>
</ul>
<% end %>